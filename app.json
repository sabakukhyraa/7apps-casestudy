{
  "expo": {
    "name": "7apps-casestudy",
    "slug": "7apps-casestudy",
    "scheme": "7appsCasestudy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sabakukhyraa.7appscasestudy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sabakukhyraa.sevenappscasestudy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Raleway-Bold.ttf",
            "./assets/fonts/Raleway-SemiBold.ttf",
            "./assets/fonts/Raleway-Medium.ttf",
            "./assets/fonts/Raleway-Regular.ttf",
            "./assets/fonts/Raleway-Light.ttf"
          ]
        }
      ],
      "expo-router",
      "expo-video"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "49509374-ee44-4202-a262-2d3cc20d1e23"
      }
    }
  }
}
